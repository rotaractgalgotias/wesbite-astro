import img5 from '../../../assets/pragati03.jpeg';
export async function getStaticPaths() {
  const events = await getCollection("project");
  return events.map((event) => {
    return {
      params: {
        slug: event.slug,
      },
      props: {
        event
      },
    };
  });
}

