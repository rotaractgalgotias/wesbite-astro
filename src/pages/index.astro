---
import Layout from "../layouts/Layout.astro";

import { HeroSlider } from "../components/hero-slider";
import { RiArrowRightSLine } from "react-icons/ri";

const sliderImages = [
  {
    image: "/cover/chav.JPG",
    id: "1",
    alt: "",
  },
  {
    image: "/cover/fellowship.jpg",
    id: "2",
    alt: "",
  },
  {
    image: "/cover/fight-hunger.JPG",
    id: "3",
    alt: "",
  },
  {
    image: "/cover/flood-relief.JPG",
    id: "4",
    alt: "",
  },
];

const eventsImages = [
  {
    image: "/cover/sapno-ki-pathshala.jpg",
    id: "1",
    alt: "",
  },
  {
    image: "/cover/seva-sahyog.JPG",
    id: "2",
    alt: "",
  },
  {
    image: "/cover/swaraj.jpg",
    id: "3",
    alt: "",
  },
